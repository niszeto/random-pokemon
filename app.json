{
  "expo": {
    "name": "random-pokemon",
    "slug": "random-pokemon",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "An app that displays a random pokemon and its information. The generation of the random range can be changed through the settings.",
    "githubUrl": "https://github.com/niszeto/random-pokemon"
  }
}